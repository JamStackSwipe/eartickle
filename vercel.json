{
  "builds": [
    { "src": "build/**", "use": "@vercel/static" },
    { "src": "api/**/*.js", "use": "@vercel/node" }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "/api/$1" },
    { "src": "/static/js/(.*)", "dest": "/build/static/js/$1" },
    { "src": "/static/css/(.*)", "dest": "/build/static/css/$1" },
    { "src": "/debug-js", "dest": "/build/static/js/main.34907665.js" },
    { "src": "/(.*)", "dest": "/build/index.html" }
  ]
}
